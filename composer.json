{
    "name": "denis-zharkov/pagesanalizer",
    "description": "Convenient service for analize your pages",
    "keywords": ["hexlet", "project", "analizer"],
    "type": "project",
    "authors": [
        {
            "name": "Zharkov Denis",
            "email": "zdu.ing@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "laravel/lumen-framework": "5.8.*",
        "vlucas/phpdotenv": "^3.3",
        "twbs/bootstrap": "^4.3.1"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.4",
        "phpunit/phpunit": "^7.0",
        "mockery/mockery": "^1.0",
        "barryvdh/laravel-debugbar": "^3.2",
        "squizlabs/php_codesniffer": "^3.4"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "phpcs": "phpcs"
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
